  <system.serviceModel>
    <extensions>
      <behaviorExtensions>
        <add name="protobuf" type="ProtoBuf.ServiceModel.ProtoBehaviorExtension, protobuf-net,PublicKeyToken=257b51d87d2e4d67"/>
