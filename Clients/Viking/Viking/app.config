      <endpoint address="https://connectomes.utah.edu/Services/Binary/Annotate.svc" binding="customBinding" bindingConfiguration="Annotation.Service.Interfaces.Generic-Binary" contract="Service.IAnnotatePermittedStructureLinks" name="Annotation.Service.Interfaces.IAnnotatePermittedStructureLinks-Binary" behaviorConfiguration="protoEndpointBehavior"/>
      <endpoint address="https://connectomes.utah.edu/Services/Binary/Annotate.svc" binding="customBinding" bindingConfiguration="Annotation.Service.Interfaces.Generic-Binary" contract="Service.IVolumeMeta" name="Annotation.Service.Interfaces.IVolumeMeta-Binary" behaviorConfiguration="protoEndpointBehavior"/>
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb"/>
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework> 
